{
  "[shellscript]": {
    "editor.formatOnSave": false
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.zwc*": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.zcompdump": true,
    "**/.zsh_history": true,
    "**/zsh/.zinit": false
  },
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/zsh/.zinit": true
  },
  "stylelint.enable": false,
  "explorerExclude.backup": null
}
