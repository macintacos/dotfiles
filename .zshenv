# Setting PATHs
export GDRIVE=$HOME/Google Drive
export ZSH=$HOME/.oh-my-zsh
export SSH_KEY_PATH=~/.ssh/rsa_id
export SCALA_HOME=/usr/local/share/scala
export GOPATH=$GDRIVE/Developer/go-workspace
export EMACSPATHVAR=$GDRIVE/Developer/_config/emacs
export PATH="$GDRIVE/Developer/go/bin:$EMACSPATHVAR:$HOME/bin:/usr/local/bin:$HOME/perl5/perlbrew/bin:/usr/local/lib/node_modules:$GOPATH/bin:$SCALA_HOME/bin:/usr/local/mysql-8.0.11-macos10.13-x86_64/bin/:$HOME/.cargo/bin:/Applications/Visual Studio Code.app/Contents/Resources/app/bin:$PATH"
# export NVM_DIR=$HOME/.nvm