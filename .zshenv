# Setting PATHs
export GDRIVE="$HOME/Google Drive"
export ZSH=$HOME/.dotfiles/oh-my-zsh
export SSH_KEY_PATH=~/.ssh/rsa_id
export SCALA_HOME=/usr/local/share/scala
export GOPATH="$GDRIVE/Developer/GoWorkspace"
export EMACSPATHVAR=$GDRIVE/Developer/_config/emacs
export NODEMODULEPATH=node_modules/.bin
export PYTHON2_PKGS=/Library/Frameworks/Python.framework/Versions/2.7/bin
export PYTHON3_PKGS=/Library/Frameworks/Python.framework/Versions/3.7/bin
export PATH="$PYTHON2_PKGS:$PYTHON3_PKGS:$NODEMODULEPATH:$GOPATH/bin:$EMACSPATHVAR:$HOME/bin:/usr/local/bin:$HOME/perl5/perlbrew/bin:/usr/local/lib/node_modules:$GOPATH/bin:$SCALA_HOME/bin:/usr/local/mysql-8.0.11-macos10.13-x86_64/bin/:$HOME/.cargo/bin:/Applications/Visual Studio Code.app/Contents/Resources/app/bin:$PATH"
